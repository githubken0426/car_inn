<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="inn.shopping.api.dao.ExpertTopMapper">

	<select id="selectByPrimaryKey" resultType="inn.shopping.api.entity.ExpertTop" parameterType="java.lang.String">
		select * from tb_expert_top
		<where>
			city_code = #{cityCode,jdbcType=VARCHAR}
			and top_title = #{categoryId,jdbcType=VARCHAR}
		</where>
	</select>
  
</mapper>